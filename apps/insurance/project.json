{
  "name": "insurance",
  "root": "apps/insurance",
  "sourceRoot": "apps/insurance/src",
  "projectType": "application",
  "tags": ["type:app"],
  "targets": {
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "vite --config apps/insurance/vite.config.ts"
      },
      "configurations": {
        "standalone": {
          "command": "cross-env VITE_STANDALONE=true vite --config apps/insurance/vite.config.ts"
        }
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "outputs": ["{workspaceRoot}/dist/apps/insurance"],
      "options": {
        "command": "vite build --config apps/insurance/vite.config.ts"
      }
    },
    "preview": {
      "executor": "nx:run-commands",
      "options": {
        "command": "vite preview --config apps/insurance/vite.config.ts"
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "eslint apps/insurance"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "vitest run --config vitest.config.ts"
      }
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc -p apps/insurance/tsconfig.json --noEmit"
      }
    }
  }
}
