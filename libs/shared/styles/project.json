{
  "name": "shared-styles",
  "root": "libs/shared/styles",
  "sourceRoot": "libs/shared/styles/src",
  "projectType": "library",
  "tags": ["type:lib", "layer:ui"],
  "targets": {
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "eslint libs/shared/styles"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "vitest run --config vitest.config.ts"
      }
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc -p libs/shared/tsconfig.json --noEmit"
      }
    }
  }
}
